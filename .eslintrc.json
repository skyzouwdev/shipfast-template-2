{
  "extends": ["next/core-web-vitals"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["import", "@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "import/order": "off",
    "react/no-unescaped-entities": "off",
    "react/prop-types": "off",
    "no-console": "warn"
  }
}
